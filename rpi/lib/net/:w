//-=Inclusion des headers=-//
#include "net/client.h"
#include "sys/log.h"
//-=Fin de la section=-//

int main(int argc, const char *argv[])
{
	if(argc < 2) return EXIT_FAILURE;

	//-=Ouverture log=-//
	syslog_open(argv[0]);
	//-=Fin de la section=-//
	

	//-=Initialisation du client=-//
	struct Client_t client = net_client_create(argv[1]);
	//-=Fin de la section=-//
	
	//-=Fermeture log=-//
	syslog_close();
	//-=Fin de la section=-//
	
	return 0;
}

